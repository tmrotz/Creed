
<div class="module-inner">


  <% if flash[:notice] %>
    <p class="notice"><%=h flash[:notice] %></p>
  <% else %>
    <p class="notice">No User Logged In</p>
  <% end %>


  <% @plans.each do |plan| %>
    <p><%=h plan.title %></p>
    <p><%=h plan.subtitle %></p>
    <p><%=h plan.target_title %></p>
    <p><%=h plan.target_body %></p>
    <p><%=h plan.financial %></p>
    <p><%=h plan.status %></p>
    <p><%=h plan.action %></p>
  <% end %>
    

  <br />


  <p class="rt-readon-surround">
    <a class="readon" href="/" >
      <span>Read More...</span>
    </a>
  </p>

</div>




