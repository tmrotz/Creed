
<div class="module-inner">

<% if flash[:notice] %>
  <p class="notice"><%=h flash[:notice] %></p>
<% end %>

  <%= link_to "Submit Plan", new_plan_url %>
  
  <br/><br/>

  <% @plans.each do |plan| %>
    <div style="margin-bottom: 12%;">
      <div style="float:left;width:15%;">
        <p class="rt-readon-surround">
          <%= link_to "Read More...", plan, :class => "readon", :style => "color:#666666;" %>
        </p>
      </div>
      <div style="float:left;width:85%;">
        <p>
          <%=h plan.title %>
          <br/>
          <%=h plan.subtitle %>
          <br/>
          <%=h plan.target_title %>
        </p>
      </div>
    </div>
  <% end %>
    


</div>




